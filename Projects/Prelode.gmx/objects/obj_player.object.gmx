        global.dir = -1; //nach links sehen
        x -= 2;
        
    }}
    
    //fuehrt die eigentliche Bewegung aus
    //scr_move_player(global.movingspeed, 0, global.dir, 0);
        

if (keyboard_check(global.up) || keyboard_check(global.down))

