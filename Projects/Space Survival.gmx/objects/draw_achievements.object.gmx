draw_text(0,48,"Achievement 2: "+GMech_AchievementTitle(2))
draw_text(0,64,"Achievement 3: "+GMech_AchievementTitle(3)) //We use GMech_AchievementTitle(), in case you ever rename an achievement later in development. It's not hard-coded.
draw_text(0,80,"Achievement 4: "+GMech_AchievementTitle(4))
draw_text(0,94,"Achievement 5: "+GMech_AchievementTitle(5))

//Now draw the ones we have
theList=""
if GMech_HasAchievement(1) then theList = string(theList)+"1, " //You should always use string() to cast strings, since GM has issues sometimes.
if GMech_HasAchievement(2) then theList = string(theList)+"2, "
if GMech_HasAchievement(3) then theList = string(theList)+"3, "
if GMech_HasAchievement(4) then theList = string(theList)+"4, "
if GMech_HasAchievement(5) then theList = string(theList)+"5"
if GMech_SignedIn() then draw_text(x,y+120,"You have achievements "+string(theList)) else draw_text(x,y+120,"Sign in to get achievements!")
</string>
          </argument>
        </arguments>
      </action>
